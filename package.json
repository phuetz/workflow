{
  "name": "workflow-automation-platform",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Enterprise-grade workflow automation platform with AI-powered features, real-time collaboration, and 500+ integrations",
  "main": "dist/main.js",
  "keywords": [
    "workflow",
    "automation",
    "n8n",
    "zapier",
    "integration",
    "enterprise",
    "react",
    "typescript",
    "nodejs",
    "collaboration",
    "ai",
    "low-code",
    "no-code"
  ],
  "author": "Workflow Automation Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/workflow-automation.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/workflow-automation/issues"
  },
  "homepage": "https://workflow-platform.com",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite --host 0.0.0.0 --port 8080",
    "dev:backend": "nodemon --watch src --ext ts,tsx --exec \"tsx --tsconfig tsconfig.dev.json src/backend/api/server.ts\"",
    "server": "node dist/src/backend/api/server.js",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:backend": "tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:performance": "artillery run tests/load/workflow-load-test.yml",
    "test:quick": "bash scripts/quick-test.sh",
    "test:health": "bash scripts/test-backend-health.sh",
    "test:frontend": "bash scripts/test-frontend.sh",
    "test:smoke": "bash scripts/smoke-tests.sh",
    "test:ci": "bash scripts/ci-validation.sh",
    "test:all": "npm run test:quick && npm run test:unit && npm run test:smoke",
    "test:auto": "concurrently --kill-others-on-fail \"npm run test:watch\" \"npm run test:smoke -- --watch\"",
    "lint": "eslint src/ --no-warn-ignored --max-warnings 3600",
    "lint:fix": "eslint src/ --no-warn-ignored --fix",
    "lint:backend": "eslint src/backend/ --no-warn-ignored --max-warnings 200",
    "lint:report": "eslint . --ext ts,tsx,js,mjs,cjs -f json -o eslint-latest.json --report-unused-disable-directives || true",
    "typecheck": "tsc --noEmit",
    "typecheck:backend": "tsc -p tsconfig.backend.json --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:credentials": "tsx scripts/migrate-credentials.ts",
    "seed": "tsx src/backend/database/seed.ts",
    "generate:keys": "tsx scripts/generate-encryption-keys.ts",
    "test:encryption": "vitest run src/__tests__/security/credentialEncryption.test.ts",
    "cli": "tsx src/backend/cli.ts",
    "studio": "prisma studio",
    "start": "node dist/src/backend/api/server.js",
    "start:prod": "NODE_ENV=production node dist/src/backend/api/server.js",
    "docker:build": "docker build -t workflow-app .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d",
    "k8s:deploy": "kubectl apply -f k8s/",
    "deploy": "./scripts/deploy.sh",
    "backup": "./scripts/backup.sh",
    "optimize": "./scripts/optimize.sh",
    "monitor": "docker-compose --profile monitoring up -d",
    "monitor:errors": "tsx scripts/monitor-and-recommend.ts",
    "monitor:demo": "tsx scripts/monitor-and-recommend.ts --demo",
    "corrections:test": "vitest run src/__tests__/correctionSystem.test.ts",
    "logs": "docker-compose logs -f",
    "clean": "rm -rf dist node_modules/.cache coverage .nyc_output",
    "analyze": "npm run build -- --analyze",
    "bundle-analyzer": "npx webpack-bundle-analyzer dist/static/js/*.js",
    "prepare": "husky"
  },
